    public ContactModel loadFromCursor(Cursor cursor) {

        this.id = cursor.getInt(ContactColumnMaps.KEY_ID);
        this.name = cursor.getString(ContactColumnMaps.NAME);
        this.phone = cursor.getString(ContactColumnMaps.PHONE);
        this.giude = cursor.getDouble(ContactColumnMaps.GIU_DE);
        this.giulo = cursor.getDouble(ContactColumnMaps.GIU_LO);
        this.giubacang = cursor.getDouble(ContactColumnMaps.GIU_BACANG);
        this.giuxien2 = cursor.getDouble(ContactColumnMaps.GIU_XIEN2);
        this.giuxien3 = cursor.getDouble(ContactColumnMaps.GIU_XIEN3);
        this.giuxien4 = cursor.getDouble(ContactColumnMaps.GIU_XIEN4);
        this.giudiemxien2 = cursor.getDouble(ContactColumnMaps.GIU_DIEM_XIEN2);
        this.giudiemxien3 = cursor.getDouble(ContactColumnMaps.GIU_DIEM_XIEN3);
        this.giudiemxien4 = cursor.getDouble(ContactColumnMaps.GIU_DIEM_XIEN4);
        this.giudiembacang = cursor.getDouble(ContactColumnMaps.GIU_DIEM_BACANG);
        this.maxDe = cursor.getDouble(ContactColumnMaps.MAX_DE);
        this.maxLo = cursor.getDouble(ContactColumnMaps.MAX_LO);
        this.maxXien2 = cursor.getDouble(ContactColumnMaps.MAX_XIEN2);
        this.maxXien3 = cursor.getDouble(ContactColumnMaps.MAX_XIEN3);
        this.maxXien4 = cursor.getDouble(ContactColumnMaps.MAX_XIEN4);
        this.maxBaCang = cursor.getDouble(ContactColumnMaps.MAX_BACANG);
        this.DAUDB = cursor.getDouble(ContactColumnMaps.DAUDB);
        this.DAUDB_LANAN = cursor.getDouble(ContactColumnMaps.DAUDB_LANAN);
        this.DITDB = cursor.getDouble(ContactColumnMaps.DITDB);
        this.DITDB_LANAN = cursor.getDouble(ContactColumnMaps.DITDB_LANAN);
        this.DAUNHAT = cursor.getDouble(ContactColumnMaps.DAUNHAT);
        this.DAUNHAT_LANAN = cursor.getDouble(ContactColumnMaps.DAUNHAT_LANAN);
        this.DITNHAT = cursor.getDouble(ContactColumnMaps.DITNHAT);
        this.DITNHAT_LANAN = cursor.getDouble(ContactColumnMaps.DITNHAT_LANAN);
        this.LO = cursor.getDouble(ContactColumnMaps.LO);
        this.LO_LANAN = cursor.getDouble(ContactColumnMaps.LO_LANAN);
        this.XIEN2 = cursor.getDouble(ContactColumnMaps.XIEN2);
        this.XIEN2_LANAN = cursor.getDouble(ContactColumnMaps.XIEN2_LANAN);
        this.XIEN3 = cursor.getDouble(ContactColumnMaps.XIEN3);
        this.XIEN3_LANAN = cursor.getDouble(ContactColumnMaps.XIEN3_LANAN);
        this.XIEN4 = cursor.getDouble(ContactColumnMaps.XIEN4);
        this.XIEN4_LANAN = cursor.getDouble(ContactColumnMaps.XIEN4_LANAN);
        this.BACANG = cursor.getDouble(ContactColumnMaps.BACANG);
        this.BACANG_LANAN = cursor.getDouble(ContactColumnMaps.BACANG_LANAN);
        this.type = cursor.getString(ContactColumnMaps.TYPE);

        return this;
    }

    public double getValue(LotoType type) {
        double field = 0;

        if (type == LotoType.de)

            field = DITDB;
        else if (type == LotoType.lo)

            field = LO;
        else if (type == LotoType.bacang)

            field = BACANG;
        else if (type == LotoType.xien2)

            field = XIEN2;
        else if (type == LotoType.xien3)

            field = XIEN3;
        else if (type == LotoType.xien4)

            field = XIEN4;

        return field;
    }

